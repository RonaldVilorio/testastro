---
const navItems = ['Pages', 'Resources', 'Blog']
---

<nav class="items-center w-full md:w-auto hidden md:flex text-gray-600 dark:text-slate-200" aria-label="Main navigation"
    id="menu">
    <ul>
        {navItems.map((item)=><li><a
                class="font-medium hover:text-gray-900 dark:hover:text-white px-4 py-3 flex items-center transition duration-150 ease-in-out"
                href="#">{item}</a></li>)}
    </ul>

</nav>